    {
        "id": "fnBuildSelect",
        "type": "function",
        "z": "1422cfa039b24723",
        "name": "build SELECT (last 100)",
        "func": "const filter = (flow.get('filter_text')||'').trim();\nlet sql = \"SELECT id, ts, topic, substr(payload,1,140) AS preview FROM events \";\nlet params = [];\nif (filter) {\n  sql += \"WHERE topic LIKE ? OR payload LIKE ? \";\n  const f = `%${filter}%`; params = [f, f];\n}\nsql += \"ORDER BY ts DESC LIMIT 100\";\nmsg.topic = sql; msg.params = params; return msg;",
        "outputs": 1,
