    {
        "id": "fnChartFormat",
        "type": "function",
        "z": "1422cfa039b24723",
        "name": "to ui-chart series",
        "func": "const rows = Array.isArray(msg.payload) ? msg.payload : [];\nconst byTopic = {};\nfor (const p of rows) {\n  const t = p.topic || \"(none)\";\n  (byTopic[t] ||= []).push({ x: new Date(p._time).getTime(), y: Number(p._value || 0) });\n}\nObject.values(byTopic).forEach(a => a.sort((a,b)=>a.x-b.x));\n\nmsg.payload = Object.entries(byTopic).map(([name, data]) => ({\n  series: [name],\n  data: [data]\n}));\nreturn msg;",
        "outputs": 1,
        "timeout": "",
        "noerr": 0,
