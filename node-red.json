    {
        "id": "3c0d0c7a0db01587",
        "type": "influxdb in",
        "z": "1422cfa039b24723",
        "influxdb": "influxCfg",
        "name": "Flux: counts by topic (1h)",
        "query": "from(bucket: \"assistant\")\n  |> range(start: -1h)\n  |> filter(fn: (r) => r._measurement == \"assistant_events\" and r._field == \"count\")\n  |> aggregateWindow(every: 1m, fn: sum)\n  |> keep(columns: [\"_time\",\"_value\",\"topic\"])\n",
        "rawOutput": false,
